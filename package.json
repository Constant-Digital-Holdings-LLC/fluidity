{
    "imports": {
        "#@shared/*": "./client/dist/public/shared/*",
        "#@client/*": "./client/dist/public/local/*",
        "#@service/*": "./service/dist/local/*"
    },
    "type": "module",
    "name": "fluidity",
    "version": "1.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "start:server": "cd service/dist/server && node --enable-source-maps app.js",
        "start:agent": "cd service/dist/agent && node --enable-source-maps app.js",
        "dev:server": "cd service/dist/server && set NODE_OPTIONS=--enable-source-maps&& set NODE_ENV=development&& concurrently -k --handle-input -n tsc-srv,nm \"tsc --build ../../tsconfig.json -w\" \"nodemon --delay 2.5 app.js\"",
        "dev:agent": "cd service/dist/agent && set NODE_OPTIONS=--enable-source-maps&& set NODE_ENV=development&& concurrently -k --handle-input -n tsc-agn,nm \"tsc --build ../../tsconfig.json -w\" \"nodemon --delay 2.5 app.js\""
    },
    "author": "Sean Lynch - KK6BEB",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.17.14",
        "@types/node": "^18.7.14",
        "@types/stack-trace": "^0.0.30",
        "concurrently": "^7.3.0",
        "nodemon": "^2.0.19",
        "typescript": "^4.8.2"
    },
    "dependencies": {
        "@types/stacktrace-js": "^2.0.3",
        "es-module-shims": "^1.5.18",
        "express": "^4.18.1",
        "ring-buffer-ts": "^1.0.3",
        "stack-trace": "^1.0.0-pre1",
        "yaml": "^2.1.1"
    }
}
